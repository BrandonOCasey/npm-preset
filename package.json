{
  "name": "npm-preset",
  "version": "1.0.1",
  "description": "Faster, sharable, and parallel npm scripts",
  "bin": {
    "npmp": "./src/npmp.js",
    "npm-preset": "./src/npmp.js"
  },
  "scripts": {
    "pretest:manual": "npm run test:clean",
    "test:manual": "node test/scripts/setup.js",
    "test:clean": "node test/scripts/clean.js",
    "prepublishOnly": "npm run test",
    "test": "nyc ava test/unit/*.test.js",
    "posttest": "npm run lint",
    "lint": "eslint -c eslint-config-videojs src test/unit, test/scripts --rule '{max-len: 0}'",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonocasey/npm-preset.git"
  },
  "author": "brandonocasey <brandonocasey@gmail.com>",
  "license": "MIT",
  "files": [
    "src/",
    "test/"
  ],
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "test"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "execa": "^0.8.0",
    "find-root": "^1.1.0",
    "intercept-stdout": "^0.1.2",
    "npm-path": "^2.0.4"
  },
  "ava": {
    "timeout": "15s",
    "require": "babel-register"
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-eslint": "^8.2.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-videojs": "^2.1.0",
    "husky": "^0.14.3",
    "nyc": "^11.4.1",
    "shelljs": "^0.7.8",
    "uuid": "^3.1.0"
  }
}
