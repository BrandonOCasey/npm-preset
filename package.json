{
  "name": "npm-preset",
  "version": "1.3.1",
  "description": "Faster, sharable, and parallel npm scripts",
  "bin": {
    "npmp": "./src/npmp.js",
    "npm-preset": "./src/npmp.js"
  },
  "scripts": {
    "prebench": "npm run test-setup",
    "bench": "node test/scripts/bench.js",
    "test-setup": "node test/scripts/setup.js",
    "prepublishOnly": "npm run test",
    "cover": "nyc ava test/unit/*.test.js --timeout '120s'",
    "test": "ava test/unit/*.test.js",
    "posttest": "npm run lint",
    "lint": "eslint -c eslint-config-videojs src test/unit test/scripts --rule '{max-len: 0}'",
    "prepush": "npm run lint",
    "version": "node scripts/version.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonocasey/npm-preset.git"
  },
  "author": "brandonocasey <brandonocasey@gmail.com>",
  "license": "MIT",
  "files": [
    "src/",
    "test/"
  ],
  "nyc": {
    "cache": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "test"
    ]
  },
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "shell-quote": "^1.6.1"
  },
  "ava": {
    "timeout": "15s"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-eslint": "^8.2.1",
    "bluebird": "^3.5.1",
    "conventional-changelog-cli": "^1.3.5",
    "conventional-changelog-videojs": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-videojs": "^2.1.0",
    "husky": "^0.14.3",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "uuid": "^3.1.0"
  }
}
